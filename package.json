{
  "name": "harmonic",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@storybook/addon-actions": "^6.2.8",
    "@storybook/addon-essentials": "^6.2.8",
    "@storybook/addon-links": "^6.2.8",
    "@storybook/addon-storyshots": "^6.2.8",
    "@storybook/addon-storyshots-puppeteer": "^6.2.8",
    "@storybook/node-logger": "^6.2.8",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.2.8",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.39",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-router-dom": "^5.1.7",
    "@types/styled-components": "^5.1.9",
    "cross-env": "^7.0.3",
    "eslint-config-standard-jsx": "^10.0.0",
    "eslint-config-standard-react": "^11.0.1",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "npm-run-all": "^4.1.5",
    "polished": "^4.1.1",
    "puppeteer": "^3.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "styled-components": "^5.2.3",
    "typescript": "^4.2.4",
    "wait-on": "^5.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:ci": "cross-env CI=true npm test",
    "test:staged": "cross-env CI=true react-scripts test --findRelatedTests",
    "test:coverage": "cross-env CI=true react-scripts test --coverage",
    "test:visual": "run-p --race snapshot:*",
    "test:visual:ci": "cross-env CI=true npm run test:visual",
    "snapshot:prepare": "run-s storybook:dev",
    "snapshot:test": "wait-on http://localhost:6006 && react-scripts test --testMatch=**/*.runner.ts src/visualTests.runner.ts",
    "eject": "react-scripts eject",
    "lint": "eslint src",
    "fix": "npm run lint -- --fix",
    "storybook:dev": "start-storybook -p 6006 -s public",
    "storybook:build": "build-storybook -s public"
  }
}
